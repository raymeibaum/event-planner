angular
  .module('eventPlanner')
  .component('userShow', {
    templateUrl: '<%= asset_path("components/user/show/userShow") %>',
    controller: UserShowController
  });

UserShowController.$inject = ['$http', '$stateParams'];

function UserShowController($http, $stateParams) {
  var vm = this;

  vm.user = [];
  vm.message = "Testing User";

  $http.get('/api/users/:id').then(function(resp) {
    console.log(resp.data);
    vm.user = resp.data;
  })

